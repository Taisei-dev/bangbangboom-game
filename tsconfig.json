{
    "compilerOptions": {
        "sourceMap": true,
        "target": "es2020",
        "module": "es2020",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "esnext"
        ],
        "types": [
            "inversify"
        ],
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,

    },
    "include": [
        "./**/*.ts"
    ],
}